{
  "name": "@hotelsoft/react-components",
  "version": "3.1.0",
  "description": "Hotelsoft Common React Components",
  "main": "dist/ReactComponents.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server1": "webpack-dev-server --content-base ./dist/ --port 8001 --colors --history-api-fallback",
    "server": "npm run devBuild & npm run httpServer & wait",
    "generateDist": "npm prune && npm run getLatestPackages && npm install && webpack && git add dist",
    "createBranch": "node ./node_modules/@hotelsoft/common/scripts/createBranch.js",
    "eslint": "eslint -c ./node_modules/@hotelsoft/common/.eslintrc src demo",
    "preinstall": "npm prune",
    "beforeCommit": "bash ./hooks/pre-commit",
    "postinstall": "npm outdated",
    "devBuild": "webpack --watch",
    "httpServer": "http-server dist -p 8081",
    "generatePull": "node ./node_modules/@hotelsoft/common/scripts/generatePullRequest.js",
    "checkIfUpdateAvailbleOnSprint": "node ./node_modules/@hotelsoft/common/scripts/isUpdateAvailable.js",
    "productionBuild": "cross-env NODE_ENV=production webpack --progress",
    "generateStates": "webpack --profile --json > stats.json",
    "getLatestPackages": "node ./node_modules/@hotelsoft/common/scripts/updateNpmPackage.js",
    "createGitLog": "git log --graph --pretty=format:'%h%  -   <%an> -   %s (%cD)' --abbrev-commit | head -n50 > ./dist/log.txt"
  },
  "files": [
    "dist/ReactComponents.css",
    "dist/ReactComponents.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hotelsoft/react-components.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Narendra Sisodiya <narendra@narendrasisodiya.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hotelsoft/react-components/issues"
  },
  "homepage": "https://github.com/hotelsoft/react-components#readme",
